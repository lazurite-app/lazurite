        "down": "N"
      }
    },
    "cameraRotation": {
      "type": "range",
      "label": "Camera Rotation",
      "value": 0,
      "min": -5,
      "max": 5,
      "keys": {
        "up": "P",
        "down": "O"
      }
    },
    "cameraHeight": {
      "type": "range",
      "label": "Camera Height",
      "value": 0,
      "min": -3,
      "max": 3,
      "keys": {
        "up": "I",
        "down": "U"
      }
    }
